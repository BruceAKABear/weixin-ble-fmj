<view class="content"><view class="operator-banner"><image class="operator-banner-pic" src="{{banner}}" mode="widthFix"></image></view><view class="goods-list"><block wx:for="{{goodsList}}" wx:for-item="goods" wx:for-index="index" wx:key="index"><view class="{{[goods.empty?styleEmpty:stylefull]}}"><view class="goods-item-inner"><view><image class="goods-item-inner-pic" src="{{goods.goods.imgUrl?goods.goods.imgUrl:defaultImg}}"></image></view><view class="goods-item-inner-name-price"><view class="goods-item-inner-name">{{''+goods.goods.name+''}}</view><view class="goods-item-inner-price">{{'￥'+goods.goods.price+''}}</view></view><view class="goods-item-inner-number"><uni-number-box vue-id="{{'1-'+index}}" value="{{0}}" min="{{0}}" product="{{goods}}" max="{{goods.stocks}}" disabled="{{goods.empty}}" data-event-opts="{{[['^change',[['selectedGoods']]]]}}" bind:change="__e" bind:__l="__l"></uni-number-box></view></view></view></block></view><view class="bottom-info"><view class="bottom-info-addcart"><view class="bottom-info-addcart-img"><image style="width:60rpx;height:60rpx;" src="../../static/cart.png"></image></view><view class="bottom-info-addcart-text">{{''+showText+''}}</view></view><view data-event-opts="{{[['tap',[['gopay',['$event']]]]]}}" class="{{[havaSelectGoods?'bottom-info-gopay-yes':'bottom-info-gopay']}}" bindtap="__e">去结算</view></view></view>